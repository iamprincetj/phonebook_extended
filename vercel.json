{
  "version": 2,
  "builds": [
    {
      "src": "phonebook",
      "use": "@vercel/static-build",
      "config": { "distDir": "public" }
    },
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/api/.*",
      "dest": "/"
    }
  ]
}
